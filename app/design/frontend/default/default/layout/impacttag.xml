<layout version="0.1.0">
    <default>
        <reference name="before_body_end">
            <block type="impacttag/impactTag" name="impacttag_tracking" template="impacttag/tracking.phtml" />
        </reference>
    </default>
    <checkout_onepage_success>
        <reference name="before_body_end">
            <block type="impacttag/impactTag" name="impacttag_conversion" template="impacttag/conversion.phtml" />
        </reference>
    </checkout_onepage_success>
    <checkout_multishipping_success>
        <reference name="before_body_end">
            <block type="impacttag/impactTag" name="impacttag_conversion" template="impacttag/conversion.phtml" />
        </reference>
    </checkout_multishipping_success>
</layout>
